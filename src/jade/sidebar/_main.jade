section#sidebar
  section#sidehead.navbar.navbar-static-top.navbar-inverse
    .navbar-inner
      .pull-right
        a.brand Company Brand
      .btn.btn-primary Join In

  section.tab-content
    include _chat
    include _users
    include _models

  section#sidefoot.navbar.navbar-static-top.navbar-inverse
    .navbar-inner
      ul.nav
        li.active: a(href="#chat", data-toggle="tab")
          i.icon-comments
        li: a(href="#users", data-toggle="tab")
          i.icon-group
        li: a(href="#models", data-toggle="tab")
          i.icon-th-large

  #toggle.btn.btn-small
    i.icon-chevron-down
